<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>David Walker, Radical Dave</Authors>
    <Company>AppBlocks, David Walker, Radical Dave</Company>
    <Description>AppBlocks for MongoDb</Description>
    <Copyright>AppBlocks, David Walker, Radical Dave</Copyright>
    <PackageProjectUrl>https://appblocks.net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/appblocksnet/appblocks.mongodbservice</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>init</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AppBlocks.Models" Version="0.0.23" />
    <PackageReference Include="MongoDB.Driver" Version="2.15.0" />
    <PackageReference Include="MongoDB.Driver.Core" Version="2.15.0" />
  </ItemGroup>

</Project>
